{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "googlekeep@fearofmice.co.uk",
      "strict_min_version": "78.0"
    }
  },
  "name": "Google Keep",
  "description": "__MSG_extensionDescription__",
  "author": "[Martin Gautier]",
  "homepage_url": "https://www.fearofmice.co.uk",
  "version": "0.20.0",
  "default_locale": "en",
  "icons": {
    "48": "skin/google_keep_icon.svg",
    "96": "skin/google_keep_icon.svg"
  },
  "browser_action": {
    "default_title": "Google Keep",
    "default_icon": "skin/google_keep_icon.svg"
  },
  "permissions": [
   "tabs",
   "storage",
   "menus",
   "webRequest",
   "webRequestBlocking",
   "https://*.keep.google.com/*"
   ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  }
}
